    "ax[2, 1].set_title('Learning Curve')\n",
    "ax[2, 1].set_xlabel('Training Examples')\n",
    "ax[2, 1].set_ylabel('Score')\n",
    "ax[2, 1].legend(loc=\"best\")\n",
    "    \n",
    "fig.tight_layout()\n"
